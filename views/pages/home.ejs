        transition: opacity 0.6s ease-out, transform 0.6s ease-out;
    }

    .scroll-fade-in.visible {
        opacity: 1;
        transform: translateY(0);
    }

    /* Stagger animation delays for child elements */
    .scroll-fade-in:nth-child(1) { transition-delay: 0.1s; }
    .scroll-fade-in:nth-child(2) { transition-delay: 0.2s; }
    .scroll-fade-in:nth-child(3) { transition-delay: 0.3s; }
    .scroll-fade-in:nth-child(4) { transition-delay: 0.4s; }
</style>

<script>
    // Smooth Scroll Animation on Page Load and Scroll
    document.addEventListener('DOMContentLoaded', function() {
        // Add scroll-fade-in class to sections
        const sections = document.querySelectorAll('.trusted-by, .features-new, .cta-section');
        sections.forEach(section => {
            section.classList.add('scroll-fade-in');
        });

        // Add scroll-fade-in to How It Works steps
        const howItWorksSteps = document.querySelectorAll('section[style*="padding: 120px 64px; background: #f9fafb"] > div > div[style*="grid-template-columns: 1fr 1fr"]');
        howItWorksSteps.forEach(step => {
            step.classList.add('scroll-fade-in');
        });

        // Add scroll-fade-in to pricing cards
        const pricingCards = document.querySelectorAll('section[style*="padding: 120px 64px; background: #fff"] > div:last-child > div');
        pricingCards.forEach(card => {
            card.classList.add('scroll-fade-in');
        });

        // Add scroll-fade-in to testimonial cards
        const testimonialCards = document.querySelectorAll('section[style*="padding: 120px 64px; background: #fafafa"] > div:last-child > div');
        testimonialCards.forEach(card => {
            card.classList.add('scroll-fade-in');
        });

        // Add scroll-fade-in to feature cards
        const featureCards = document.querySelectorAll('.feature-card-new');
        featureCards.forEach(card => {
            card.classList.add('scroll-fade-in');
        });

        // Intersection Observer for scroll animations
        const observerOptions = {
            threshold: 0.1,
            rootMargin: '0px 0px -50px 0px'
        };

        const observer = new IntersectionObserver(function(entries) {
            entries.forEach(entry => {
                if (entry.isIntersecting) {
                    entry.target.classList.add('visible');
                }
            });
        }, observerOptions);

        // Observe all elements with scroll-fade-in class
        document.querySelectorAll('.scroll-fade-in').forEach(element => {
            observer.observe(element);
        });

        // Smooth scroll for anchor links
        document.querySelectorAll('a[href^="#"]').forEach(anchor => {
            anchor.addEventListener('click', function(e) {
                const href = this.getAttribute('href');
                if (href !== '#' && href.length > 1) {
                    e.preventDefault();
                    const target = document.querySelector(href);
                    if (target) {
                        const navbarHeight = document.getElementById('navbar')?.offsetHeight || 72;
                        const targetPosition = target.offsetTop - navbarHeight;
                        window.scrollTo({
                            top: targetPosition,
                            behavior: 'smooth'
                        });
                    }
                }
            });
        });

        // Parallax effect for hero visual frames (subtle)
        const visualFrames = document.querySelectorAll('.visual-frame');
        let ticking = false;

        window.addEventListener('scroll', function() {
            if (!ticking) {
                window.requestAnimationFrame(function() {
                    const scrolled = window.pageYOffset;
                    visualFrames.forEach((frame, index) => {
                        const speed = 0.05 * (index + 1);
                        const yPos = -(scrolled * speed);
                        frame.style.transform = `translateY(${yPos}px)`;
                    });
                    ticking = false;
                });
                ticking = true;
            }
        });
    });
</script>

