<%- include('../partials/header') %>

<style>
    /* Ensure navbar is visible */
    #navbar {
        position: fixed !important;
        top: 0 !important;
        left: 0 !important;
        right: 0 !important;
        z-index: 10000 !important;
        background: rgba(250, 250, 250, 0.95) !important;
        backdrop-filter: blur(10px) !important;
        border-bottom: 1px solid rgba(0, 0, 0, 0.08) !important;
    }

    #navbar .logo-text,
    #navbar .nav-links > a,
    #navbar .nav-links > .nav-dropdown > button,
    #navbar .nav-login {
        color: #1a1a1a !important;
    }

    #navbar .logo-frame {
        border-color: #1a1a1a !important;
    }

    #navbar .nav-cta {
        background: #1a1a1a !important;
        color: #fff !important;
    }

    #navbar .nav-links > .nav-dropdown > button svg path {
        stroke: #1a1a1a !important;
    }

    /* Override body background */
    body {
        background: #fafafa;
        padding-top: 0;
    }

    /* Hero Section */
    .hero-new {
        max-width: 1200px;
        margin: 0 auto;
        padding: 140px 64px 120px;
        display: grid;
        grid-template-columns: 1fr 1fr;
        gap: 80px;
        align-items: center;
        min-height: calc(100vh - 100px);
        position: relative;
        z-index: 1;
    }

    .hero-content-new {
        position: relative;
    }

    .status-badge {
        display: inline-flex;
        align-items: center;
        gap: 10px;
        font-family: 'JetBrains Mono', monospace;
        font-size: 12px;
        color: #2563eb;
        margin-bottom: 32px;
        padding: 10px 16px;
        border: 1px solid #2563eb;
        position: relative;
    }

    .status-badge::before {
        content: '';
        width: 8px;
        height: 8px;
        background: #2563eb;
        animation: blink 1.5s infinite;
    }

    @keyframes blink {
        0%, 100% { opacity: 1; }
        50% { opacity: 0.3; }
    }

    .hero-title-new {
        font-family: 'Syne', sans-serif;
        font-size: 64px;
        font-weight: 800;
        line-height: 1.05;
        letter-spacing: -0.03em;
        margin-bottom: 28px;
        color: #1a1a1a;
    }

    .highlight-frame {
        position: relative;
        display: inline-block;
        color: #2563eb;
    }

    .highlight-frame::before,
    .highlight-frame::after {
        content: '';
        position: absolute;
        width: 12px;
        height: 12px;
        border: 2px solid #2563eb;
    }

    .highlight-frame::before {
        top: -8px;
        left: -12px;
        border-right: none;
        border-bottom: none;
    }

    .highlight-frame::after {
        bottom: -8px;
        right: -12px;
        border-left: none;
        border-top: none;
    }

    .hero-desc-new {
        font-size: 18px;
        color: #6b7280;
        line-height: 1.8;
        max-width: 440px;
        margin-bottom: 48px;
    }

    .hero-cta-group {
        display: flex;
        gap: 16px;
        margin-bottom: 32px;
    }

    .btn-primary-new {
        padding: 16px 32px;
        background: #1a1a1a;
        color: #fff;
        text-decoration: none;
        font-size: 15px;
        font-weight: 600;
        transition: all 0.2s;
        display: inline-flex;
        align-items: center;
        gap: 8px;
    }

    .btn-primary-new:hover {
        background: #2563eb;
    }

    .btn-secondary-new {
        padding: 16px 32px;
        background: transparent;
        color: #1a1a1a;
        text-decoration: none;
        font-size: 15px;
        font-weight: 600;
        border: 2px solid #1a1a1a;
        transition: all 0.2s;
        display: inline-flex;
        align-items: center;
        gap: 8px;
    }

    .btn-secondary-new:hover {
        background: #1a1a1a;
        color: #fff;
    }

    .hero-stats {
        display: flex;
        gap: 32px;
        font-size: 13px;
        color: #6b7280;
    }

    .hero-stats strong {
        color: #1a1a1a;
        font-weight: 600;
    }

    /* Visual Frame Stack */
    .frame-visual {
        position: relative;
        height: 480px;
        z-index: 1;
    }

    .visual-frame {
        position: absolute;
        background: #fff;
        border: 2px solid #e5e5e5;
        transition: all 0.4s cubic-bezier(0.4, 0, 0.2, 1);
        border-radius: 12px;
        overflow: hidden;
    }

    .visual-frame:hover {
        border-color: #2563eb;
        transform: translate(-8px, -8px);
        box-shadow: 8px 8px 0 #2563eb;
        z-index: 10;
    }

    .frame-header {
        padding: 12px 16px;
        border-bottom: 1px solid #e5e5e5;
        display: flex;
        align-items: center;
        gap: 8px;
    }

    .frame-dot {
        width: 8px;
        height: 8px;
        border-radius: 50%;
        background: #e5e5e5;
    }

    .frame-title {
        font-family: 'JetBrains Mono', monospace;
        font-size: 11px;
        color: #6b7280;
        margin-left: auto;
    }

    .frame-body {
        padding: 20px;
    }

    /* Frame 1: Live Dashboard */
    .vf-1 {
        top: 0;
        left: 0;
        width: 280px;
        z-index: 3;
    }

    /* Frame 2: Performance Metrics */
    .vf-2 {
        top: 100px;
        right: 0;
        width: 260px;
        z-index: 2;
    }

    /* Frame 3: Recent Activity */
    .vf-3 {
        bottom: 20px;
        left: 60px;
        width: 300px;
        z-index: 1;
    }

    /* Features Section */
    .features-new {
        padding: 100px 64px;
        background: #1a1a1a;
        color: #fff;
        position: relative;
    }

    .features-inner {
        max-width: 1200px;
        margin: 0 auto;
    }

    .features-header-new {
        display: flex;
        justify-content: space-between;
        align-items: flex-end;
        margin-bottom: 64px;
    }

    .features-title-new {
        font-family: 'Syne', sans-serif;
        font-size: 48px;
        font-weight: 800;
        letter-spacing: -0.02em;
    }

    .features-subtitle {
        color: rgba(255,255,255,0.5);
        font-size: 14px;
    }

    .features-grid-new {
        display: grid;
        grid-template-columns: repeat(4, 1fr);
        gap: 2px;
        background: rgba(255,255,255,0.1);
    }

    .feature-card-new {
        background: #1a1a1a;
        padding: 40px 32px;
        transition: background 0.3s;
    }

    .feature-card-new:hover {
        background: #2a2a2a;
    }

    .feature-number {
        font-family: 'JetBrains Mono', monospace;
        font-size: 12px;
        color: #2563eb;
        margin-bottom: 24px;
    }

    .feature-title-new {
        font-family: 'Syne', sans-serif;
        font-size: 20px;
        font-weight: 700;
        margin-bottom: 12px;
    }

    .feature-desc-new {
        font-size: 14px;
        color: rgba(255,255,255,0.5);
        line-height: 1.7;
    }

    /* Trusted By Section */
    .trusted-by {
        padding: 80px 64px;
        text-align: center;
        background: #fafafa;
    }

    .trusted-by-label {
        font-family: 'JetBrains Mono', monospace;
        font-size: 11px;
        color: #6b7280;
        text-transform: uppercase;
        letter-spacing: 0.15em;
        margin-bottom: 32px;
    }

    .trusted-logos {
        display: flex;
        justify-content: center;
        align-items: center;
        gap: 48px;
        flex-wrap: wrap;
        opacity: 0.4;
    }

    .trusted-logos span {
        font-family: 'Syne', sans-serif;
        font-size: 24px;
        font-weight: 700;
        color: #1a1a1a;
    }

    /* CTA Section */
    .cta-section {
        padding: 120px 64px;
        text-align: center;
        background: #2563eb;
        color: #fff;
    }

    .cta-title {
        font-family: 'Syne', sans-serif;
        font-size: 48px;
        font-weight: 800;
        margin-bottom: 20px;
    }

    .cta-desc {
        font-size: 18px;
        color: rgba(255,255,255,0.8);
        margin-bottom: 40px;
    }

    .cta-btn {
        padding: 18px 40px;
        background: #fff;
        color: #2563eb;
        text-decoration: none;
        font-size: 16px;
        font-weight: 600;
        display: inline-flex;
        align-items: center;
        gap: 10px;
        transition: all 0.2s;
    }

    .cta-btn:hover {
        background: #1a1a1a;
        color: #fff;
    }

    /* Responsive */
    @media (max-width: 1000px) {
        .hero-new {
            grid-template-columns: 1fr;
            gap: 60px;
            padding-top: 120px;
        }

        .frame-visual {
            height: 360px;
        }

        .vf-1 { width: 260px; }
        .vf-2 { right: auto; left: 180px; top: 80px; width: 240px; }
        .vf-3 { bottom: 0; left: 40px; width: 280px; }

        .features-grid-new {
            grid-template-columns: 1fr 1fr;
        }
    }

    @media (max-width: 768px) {
        .hero-new { padding: 100px 24px 80px; }
        .hero-title-new { font-size: 40px; }

        .frame-visual { display: none; }

        .hero-cta-group { flex-direction: column; }

        .features-new { padding: 60px 24px; }
        .features-grid-new { grid-template-columns: 1fr; }
        .features-header-new { flex-direction: column; align-items: flex-start; gap: 8px; }

        .trusted-by { padding: 60px 24px; }
        .cta-section { padding: 80px 24px; }
    }

    /* Pricing Responsive */
    @media (max-width: 1000px) {
        section[style*="grid-template-columns: repeat(3, 1fr)"] {
            grid-template-columns: 1fr !important;
        }
        
        div[style*="transform: scale(1.05)"] {
            transform: scale(1) !important;
        }
    }

    /* Testimonials Responsive */
    @media (max-width: 1000px) {
        section div[style*="grid-template-columns: repeat(3, 1fr)"] {
            grid-template-columns: 1fr !important;
        }
    }

    /* How It Works Responsive */
    @media (max-width: 1000px) {
        section div[style*="grid-template-columns: 1fr 1fr"] {
            grid-template-columns: 1fr !important;
        }
        
        section div[style*="order: 1"] {
            order: 2 !important;
        }
        
        section div[style*="order: 2"] {
            order: 1 !important;
        }
    }

    @media (max-width: 600px) {
        section div[style*="grid-template-columns: repeat(4, 1fr)"],
        section div[style*="grid-template-columns: repeat(2, 1fr)"] {
            grid-template-columns: 1fr !important;
        }
    }
</style>

